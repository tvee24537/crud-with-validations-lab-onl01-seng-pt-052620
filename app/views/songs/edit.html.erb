<%= form_for(@song) do |f|%>
<p>  <%= f.label :title %>
  <%= f.text_field :title %></p>
<p>  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %></p>
<p>  <%= f.label :Genre %>
  <%= f.text_field :genre %></p>
<p><%= f.label :released %>
<%= f.check_box :released %></p>
<p>  <%= f.text_field :release_year %></p>
<p>  <%= f.submit%></p>
<% end %> 